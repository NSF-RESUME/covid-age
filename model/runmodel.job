#!/bin/bash
#SBATCH -A b1139
#SBATCH -p b1139
#SBATCH -t 03:00:00
#SBATCH --job-name="covid-age"
#SBATCH --mem-per-cpu=4G
#SBATCH --array=1-10
#SBATCH --error=log/arrayJob_%A_%a.err
#SBATCH --output=log/arrayJob_%A_%a.out

offset=7000
nloops=50
((ini=(${SLURM_ARRAY_TASK_ID}-1)*$nloops+1+$offset))
((fin=$ini+$nloops-1))

for (( i=$ini; i<=$fin; i++))
  do
    ./model $i
  done
